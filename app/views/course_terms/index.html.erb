<p id="notice"><%= notice %></p>

<h1>Listing Course Terms</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_terms.each do |course_term| %>
      <tr>
        <td><%= link_to 'Show', course_term %></td>
        <td><%= link_to 'Edit', edit_course_term_path(course_term) %></td>
        <td><%= link_to 'Destroy', course_term, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course term', new_course_term_path %>
